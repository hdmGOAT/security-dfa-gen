digraph PDA {
  __start [shape=point];
  q0 [label="q0"];
  q1 [label="q1", shape=doublecircle];

  __start -> q0 [label="ε, ε -> Z0"];

  // On 'a' push A (no pop)
  q0 -> q0 [label="a, ε -> A"];

  // When b encountered, pop A; move to q1 to consume bs
  q0 -> q1 [label="b, A -> ε"];
  q1 -> q1 [label="b, A -> ε"];
}
